    location / {
      proxy_pass http://$local_endpoint;
    }

    location /cms {
      rewrite ^/cms/(.*)$ /$1 break;
      proxy_pass http://$cms_endpoint;
    }

    location /api {
      proxy_pass http://$api_endpoint;
    }

    location /portkey {
      rewrite ^/portkey/(.*)$ /$1 break;
      proxy_pass http://$portkey_api_endpoint;
    }

    location /connect {
      proxy_pass http://$portkey_auth_endpoint/connect;
    }

    location /AElfIndexer_DApp/PortKeyIndexerCASchema {
      proxy_pass http://$portkey_dapp_endpoint/AElfIndexer_DApp/PortKeyIndexerCASchema;
    }

    location /AElfIndexer_BeangoTown/BeangoTownIndexerPluginSchema {
      proxy_pass http://$beangotown_dapp_endpoint/AElfIndexer_BeangoTown/BeangoTownIndexerPluginSchema;
    }